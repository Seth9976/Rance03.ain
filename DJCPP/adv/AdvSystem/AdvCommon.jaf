void RunPartsMotion()
{
	while (!g_partsMotionManager.isEmpty())
	{
		g_frameManager.updateFrame();
		if (既読スキップ中確認() || 全文スキップ中確認())
		{
			g_partsMotionManager.finalize(-1);
			g_frameManager.updateFrame();
		}
	}
}

