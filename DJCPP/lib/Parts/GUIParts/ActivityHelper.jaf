void getPartsNameByGUIType(ref array@string partsName, string activityName, int guiType)
{
	partsName.Free();
	int count = AFL_Activity_NumofParts(activityName);
	int i;
	for (i = 0; i < count; ++i)
	{
		int number;
		string name;
		AFL_Activity_GetParts(i, activityName, name, number);
		if (PARTS_GetComponentType(number, 1) == guiType)
		{
			partsName.PushBack(name);
		}
	}
}

