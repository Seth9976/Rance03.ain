				ＥＶ／ガーネット／ラレラレ石Ｈ();
				ＥＶ／自宅／観る／ガーネット／Ｈ後();
				制御／アイテム削除("ラレラレ石2", 1, true);
			}
			break;
		case 2:
			if (!EV("ＥＶ／自宅／寝る／初回", -2147483648))
			{
				ＥＶ／自宅／寝る／初回();
			}
			else
			{
