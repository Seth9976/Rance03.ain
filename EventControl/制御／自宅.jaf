				ＥＶ／汎用／出る();
			}
			else if (EV("ＥＶ／自宅／話す／三回目", -2147483648) && !EV("ＥＶ／自宅／襲う／彫像", -2147483648))
			{
				ＥＶ／自宅／襲う／彫像();
				制御／寝る(false);
				ＥＶ／汎用／出る();
