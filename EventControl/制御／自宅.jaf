			}
			制御／寝る(false);
			ＥＶ／汎用／出る();
			return;
		case 3:
			if (!EV("ＥＶ／自宅／話す／初回", -2147483648))
			{
				ＥＶ／自宅／話す／初回();
			}
			else if (!EV("ＥＶ／自宅／話す／二回目", -2147483648))
			{
