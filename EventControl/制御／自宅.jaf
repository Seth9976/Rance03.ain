			}
			else if (!EV("ＥＶ／自宅／話す／三回目", -2147483648))
			{
				ＥＶ／自宅／話す／三回目();
			}
			else
			{
				ＥＶ／自宅／話す／汎用();
			}
			break;
		case 4:
			if (!EV("ＥＶ／自宅／襲う／初回", -2147483648))
			{
				ＥＶ／自宅／襲う／初回();
				ＥＶ／シィル／台所Ｈ／二回目();
				ＥＶ／自宅／襲う／初回／Ｈ後();
