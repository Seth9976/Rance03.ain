		制御／選択肢(4, "なんか襲う");
		制御／選択肢(99, "ここを出る");
		switch (制御／選択肢番号(false))
		{
		case 1:
			if (制御／アイテム確認("ラレラレ石1"))
			{
				ＥＶ／自宅／観る／サファイア();
				ＥＶ／サファイア／ラレラレ石Ｈ();
				ＥＶ／自宅／観る／サファイア／Ｈ後();
