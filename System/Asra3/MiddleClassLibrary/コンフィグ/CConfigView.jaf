void CConfigView::Execute(string ActivityNameByGame)
{
	this.m_Begin();
	bool IsExistActivityByGame = SYS_Activity_IsExistFile(ActivityNameByGame);
	bool IsExistDefaultActivity = SYS_Activity_IsExistFile("Asra\\コンフィグ／０１／メッセージ速度ページ");
	string FileName;
	if (IsExistActivityByGame)
	{
		FileName = ActivityNameByGame;
	}
	else if (system.IsDebugMode() && IsExistDefaultActivity)
	{
		FileName = "Asra\\コンフィグ／０１／メッセージ速度ページ";
	}
	Ｐ＿アクティビティ実行(FileName, this.LoadedActivityEvent, this.ReleasingActivityEvent, NULL);
	this.m_ActivityNameList.Free();
	this.m_End();
}

