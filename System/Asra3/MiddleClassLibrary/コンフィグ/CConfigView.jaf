void CConfigView::CallFunction(int Number, string PartsName, DG_NotifyHandler dgObject)
{
	int i;
	for (i = 0; i < this.m_ActivityNameList.Numof(); ++i)
	{
		if (Number != AFL_Activity_GetPartsNumber(this.m_ActivityNameList[i], PartsName))
		{
			continue;
		}
		dgObject();
	}
}

