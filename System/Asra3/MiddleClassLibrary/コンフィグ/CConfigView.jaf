void CConfigView::ReleasingActivityEvent(string ActivityFileName, string ActivityName)
{
	int Index = this.m_ActivityNameList.Find(0, this.m_ActivityNameList.Numof(), ActivityName);
	if (Index == -1)
	{
		return;
	}
	this.m_ActivityNameList.Erase(Index);
	if (this.m_ActivityNameList.Empty())
	{
		AFL_Parts_EraseEndUpdateEvent(this.EndUpdateEvent);
		AFL_Parts_EraseBeginUpdateEvent(this.BeginUpdateEvent);
	}
	this.m_dgReleasingObject(ActivityFileName, ActivityName);
}

void CConfigView::AddButtonEvent(string ActivityName, string PartsName)
{
	int Number = AFL_Activity_GetPartsNumber(ActivityName, PartsName);
	if (Number == 0)
	{
		return;
	}
	AFL_Parts_AddMouseLClickEvent(Number, this.ClickEvent);
}

