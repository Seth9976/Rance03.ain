void CConfigView::SetHScrollRateForAsra(string PartsName, DG_SetFloatHandler dgObject)
{
	int i;
	for (i = 0; i < this.m_ActivityNameList.Numof(); ++i)
	{
		int Number = AFL_Activity_GetPartsNumber(this.m_ActivityNameList[i], PartsName);
		if (Number == 0)
		{
			continue;
		}
		dgObject(Ｐ＿横スクロールバー＿スクロールレート取得(Number));
	}
}

void CConfigView::SetCheckStatForAsra(string PartsName, DG_SetBoolHandler dgObject)
{
	int i;
	for (i = 0; i < this.m_ActivityNameList.Numof(); ++i)
	{
		int Number = AFL_Activity_GetPartsNumber(this.m_ActivityNameList[i], PartsName);
		if (Number == 0)
		{
			continue;
		}
		dgObject(Ｐ＿チェックボックス＿チェック状態取得(Number));
	}
}

