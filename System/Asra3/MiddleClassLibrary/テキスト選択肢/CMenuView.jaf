void CMenuView::_CreateItems(ref 選択エリア_t _area)
{
	this._m_item.Free();
	this._m_item.Alloc(_選択肢.Numof());
	int _i;
	for (_i = 0; _i < _選択肢.Numof(); ++_i)
	{
		this._m_item[_i]._Create(_選択肢[_i], this._m_tm1, this._m_tm2, this._m_tm3, this.m_fBoldWeight, this.m_fSelectedBoldWeight, this.m_fUnselectableBoldWeight, this.m_nCharSpace, this.m_nSelectedCharSpace, this.m_nUnselectableCharSpace, 2, 32768, (this._m_ox + _area.余白左) - 1, (this._m_oy + _area.余白上 + _i * (_area.文字サイズ + _area.行間)) - 1, this._m_total_w);
	}
	if (system.IsDebugMode())
	{
		this.m_SelectClick.Alloc(_選択肢.Numof());
		int i;
		for (i = 0; i < this.m_SelectClick.Numof(); i++)
		{
			if (_選択肢[i].キーコード == 0)
			{
				continue;
			}
			this.m_SelectClick[i].Init(_選択肢[i].キーコード, true);
		}
	}
}

