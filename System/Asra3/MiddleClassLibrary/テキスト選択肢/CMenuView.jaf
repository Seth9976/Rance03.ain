}

void CMenuView::_Loop(bool _キャンセルあり, int _キャンセル値)
{
	while (!this._m_exit)
	{
		this._Update(_キャンセルあり, _キャンセル値);
	}
}

void CMenuView::_Update(bool _キャンセルあり, int _キャンセル値)
{
	int MouseX;
	int MouseY;
	AFL_Mouse_GetPos(MouseX, MouseY);
	bool MouseMoved = MouseX != this.m_PrevMouseX || MouseY != this.m_PrevMouseY;
	this.m_PrevMouseX = MouseX;
	this.m_PrevMouseY = MouseY;
	int s = this._m_sel_item;
	if (MouseMoved)
	{
		s = this._ComputeCurSelByMouse();
	}
	else
	{
		s = this._ComputeCurSelByKey();
	}
	this._UpdateItem(s);
	g_SystemButtonViewList.Update();
	sys_btn_fnc_t _fn = this._m_result.fnc;
	this._m_result.fnc = g_SystemButtonViewList.GetSelectedFunc();
	if (this._m_result.fnc.Numof() != 0)
	{
		s = -2147483648;
	}
	if (!SYS_IsSame_sys_btn_fnc_t(this._m_result.fnc, _fn) && this._m_result.fnc.Numof() != 0)
	{
		g_SystemButtonViewList.PlayOnCursorSound();
	}
	if (s != this._m_sel_item && s != -2147483648)
	{
		if (this._m_move == 1)
		{
			if (!_選択エリア[_選択枠].音移動１.Empty())
			{
				システム効果音(_選択エリア[_選択枠].音移動１);
			}
			if (!_選択エリア[_選択枠].音移動２.Empty())
			{
				this._m_move = 2;
			}
		}
		else
		{
			if (!_選択エリア[_選択枠].音移動２.Empty())
			{
				システム効果音(_選択エリア[_選択枠].音移動２);
			}
			if (!_選択エリア[_選択枠].音移動１.Empty())
			{
				this._m_move = 1;
			}
		}
	}
	AFL_View_Update(true);
	if (SYS_ホイールフォア即確認())
	{
		this._m_result.fnc = &SYS_RunCommandMouseWheelForward;
		this._m_exit = true;
		return;
	}
	if (this.m_PageUpKeyClick.IsClick(-2147483648))
	{
		this._m_result.fnc = &Ｓ＿バックログ;
		this._m_exit = true;
		return;
	}
	if (this.m_HomeKeyClick.IsClick(-2147483648))
	{
		this._m_result.fnc = &Ｓ＿バックシーン;
		this._m_exit = true;
		return;
	}
	if (this.m_EscapeKeyClick.IsClick(-2147483648))
	{
		this._m_result.fnc = &Ｓ＿コンフィグ;
		this._m_exit = true;
		return;
	}
	this._m_sel_item = s;
	if (this.m_LButtonClick.IsClick(s) || this.m_EnterKeyClick.IsClick(s))
	{
		if (this._m_result.fnc.Numof() != 0)
		{
			g_SystemButtonViewList.PlayClickSound();
			this._SetResultValue();
			this._m_exit = true;
			return;
		}
		if (s != -2147483648)
		{
			if (!_選択エリア[_選択枠].音決定.Empty())
			{
				システム効果音(_選択エリア[_選択枠].音決定);
			}
			this._SetResultValue();
			this._m_exit = true;
			return;
		}
		if (!_選択エリア[_選択枠].音無効.Empty())
		{
			システム効果音(_選択エリア[_選択枠].音無効);
		}
	}
	if (this.m_RButtonClick.IsClick(-2147483648) || this.m_SpaceKeyClick.IsClick(-2147483648))
	{
		if (_キャンセルあり)
		{
			this._m_result.値 = _キャンセル値;
			this._m_exit = true;
			return;
		}
		else
		{
			_g_menu_rbutton_callback_fnc();
		}
	}
	if (system.IsDebugMode())
	{
		int i;
		for (i = 0; i < this.m_SelectClick.Numof(); i++)
		{
			if (this.m_SelectClick[i].IsClick(-2147483648))
			{
				this._m_sel_item = i;
				this._SetResultValue();
				this._m_exit = true;
				return;
			}
		}
	}
}

